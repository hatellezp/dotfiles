!!!!!!!!!
! URxvt !
!!!!!!!!!
URxvt.color12: rgb:5c/5c/ff

URxvt.font: xft:Fira Code Retina:style=Retina:pixelsize=16
URxvt.jumpScroll: true
URxvt.perl-ext: reactivity
URxvt.perl-ext-common: keyboard-select,clipboard,url-select,matcher

URxvt.keysym.M-m: perl:reactivity:mon
URXVT.keysym.M-0: perl:reactivity:resetfont
URxvt.keysym.C-minus: perl:reactivity:smallerfont
URxvt.keysym.C-plus: perl:reactivity:largerfont
URxvt.keysym.M-equal: perl:keyboard-select:activate
URxvt.keysym.M-U: perl:url-select:select_next
URxvt.keysym.Shift-Control-C:     perl:clipboard:copy
URxvt.keysym.Shift-Control-V:     perl:clipboard:paste
! URxvt.keysym.Control-y:           perl:clipboard:paste
URxvt.keysym.M-C-v:               perl:clipboard:paste_escaped

URxvt.url-select.launcher:  firefox
URxvt.url-select.underline: true
URxvt.clipboard.autocopy: true
URxvt.clipboard.copycmd:  xclip -i -selection clipboard
URxvt.clipboard.pastecmd: xclip -o -selection clipboard
URxvt.keyboard-select.clipboard: true

URxvt.matcher.rend.0: Uline Bold fg6

URxvt.depth:                      32
URxvt.background:                 [95]#222D31
URxvt*scrollBar:                  false
URxvt*mouseWheelScrollPage:       true
URxvt*cursorBlink:                true
! URxvt*background:                 black
URxvt*foreground:                 grey
URxvt*saveLines:                  10000


URxvt.iso14755:                   false
URxvt.iso14755_52:                false


!Xterm escape codes, word by word movement
URxvt.keysym.Control-Left:        \033[1;5D
URxvt.keysym.Shift-Control-Left:  \033[1;6D
URxvt.keysym.Control-Right:       \033[1;5C
URxvt.keysym.Shift-Control-Right: \033[1;6C
URxvt.keysym.Control-Up:          \033[1;5A
URxvt.keysym.Shift-Control-Up:    \033[1;6A
URxvt.keysym.Control-Down:        \033[1;5B
URxvt.keysym.Shift-Control-Down:  \033[1;6B
URxvt.keysym.Shift-Up: command:\033]720;1\007
URxvt.keysym.Shift-Down: command:\033]721;1\007
! URxvt.keysym.Control-l: command:\033c

